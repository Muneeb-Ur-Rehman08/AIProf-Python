{
    "version": 2,
    "builds": [
        {
            "src": "app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12.7",
                "maxLambdaSize": "20mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/wsgi.py"
        }
    ]
}